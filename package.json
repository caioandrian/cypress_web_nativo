{
  "name": "cypress_e2e",
  "version": "1.0.0",
  "description": "Projeto de automação de testes E2E para o site Saucedemo",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:open-prod": "cypress open --env fileConfig=prod",
    "cy:run-saucedemo": "cypress run --spec 'cypress/e2e/spec/exemplos/saucedemo.cy.js' --browser chrome --headless",
    "cypress:verify": "cypress verify",
    "update-packages": "npx npm-check-updates"
  },
  "keywords": [
    "cypress",
    "e2e",
    "automation",
    "test",
    "saucedemo"
  ],
  "author": "Caio Andrian",
  "license": "MIT",
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "cypress": "^14.4.1",
    "mocha": "^10.8.2"
  }
}
