    "cypress-axe": "^1.6.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-delete-downloads-folder": "^0.0.6",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^2.0.1",
    "eslint": "^9.15.0",
    "eslint-plugin-cucumber": "^2.0.0",
